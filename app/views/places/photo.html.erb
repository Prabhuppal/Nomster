<div class="clear"></div>
<div class="booyah-box col-10 offset-1 ">
  <h1>Add a Photo</h1>
    <%= simple_form_for @photo, url: place_photos_path(@place) do |f| %>
    <%= f.input :caption %>
    <%= f.input :picture %>
    <br />
    <%= f.submit 'Add a Photo', class: 'btn btn-primary' %>
  <% end %>
</div>